{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Osiris",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#logical"
		},
		{
			"include": "#support"
		},
		{
			"include": "#entity"
		},
		{
			"include": "#meta"
		},
		{
			"include": "#comment"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.database.osiris",
				"match": "\\bDB_(\\w)+"
			},{
				"name": "keyword.database.osiris",
				"match": "\\bPROC\n"
			}]
		},
		"comment": {
			"patterns": [{
				"name": "comment",
				"match": "\/\/(.*)?\n"
			}]
		},
		"entity": {
			"patterns": [{
				"name": "entity.name.function.osiris",
				"begin": "(?<=PROC\n)",
				"end": "(?=\\()"
			}]
		},
		"support": {
			"patterns": [{
				"name": "support.function.osiris",
				"match": "\\b\\w+(?=\\()"
			}]
		},
		"logical": {
			"patterns": [{
				"name": "logical.osiris",
				"match": "(AND|THEN)"
			}]
		},
		"strings": {
			"name": "string.quoted.double.osiris",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.osiris",
					"match": "\\\\."
				}
			]
		},
		"meta":{
			"patterns": [
				{
					"name": "meta.section.osiris",
					"match": "INITSECTION"
				}
			]
		}
	},
	"scopeName": "source.osiris"
}